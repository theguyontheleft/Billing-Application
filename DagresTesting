echo "Testing adding a correct billing entries"
echo "************************************"
$prog -add -n "John Smith" -d "03/22/11" -s "10:11" -e "10:12"
echo "************************************"
echo "Testing incorrect -add command"
echo "************************************"
$prog -ads -n "John Tyler" -d "03/23/11" -s "10:11" -e "10:12"
echo "************************************"
echo "Testing incorrect -n command"
echo "************************************"
$prog -add -4 "amos west" -d "03/23/11" -s "10:13" -e "10:14"
echo "************************************"
echo "Testing incorrect -d command"
echo "************************************"
$prog -add -n "amos west" -g "03/23/11" -s "10:13" -e "10:14"
echo "************************************"
echo "Testing incorrect -date month command"
echo "************************************"
$prog -add -n "amos west" -d "13/23/11" -s "10:13" -e "10:14"
